<?xml version="1.0"?>
<launch>
  <arg name="urdf_file" default="pallet_truck_std.urdf.xacro"/>
  <arg name="prefix" default="pallet_truck/"/>
	
  <param name="robot_description" command="$(find xacro)/xacro '$(find pallet_truck_description)/robots/$(arg urdf_file)' prefix:=$(arg prefix)"/>
	
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
</launch>
